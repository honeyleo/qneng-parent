<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.lfy.qneng.dao.ScheduleJobDAO" >
  <resultMap id="BaseResultMap" type="cn.lfy.common.job.model.ScheduleJob" >
    <id column="id" property="jobId" jdbcType="BIGINT" />
    <result column="jobName" property="jobName" jdbcType="VARCHAR" />
    <result column="jobGroup" property="jobGroup" jdbcType="VARCHAR" />
    <result column="jobStatus" property="jobStatus" jdbcType="VARCHAR" />
    <result column="cronExpression" property="cronExpression" jdbcType="VARCHAR" />
    <result column="desc" property="desc" jdbcType="VARCHAR" />
    <result column="operator" property="operator" jdbcType="BIGINT" />
    <result column="opTime" property="opTime" jdbcType="BIGINT" />
    <result column="concurrent" property="concurrent" jdbcType="BOOLEAN" />
    <result column="idc" property="idc" jdbcType="VARCHAR" />
    <result column="isBoot" property="isBoot" jdbcType="BOOLEAN" />
  </resultMap>
  <sql id="Example_Where_Clause" >
    <trim prefix="where" prefixOverrides="and|or" >
      <if test="condition.no != null" >
         and no = #{condition.no}
      </if>
      <if test="condition.time != null" >
         and time >= #{condition.time}
      </if>
    </trim>
  </sql>
  
  <sql id="Example_Order_Clause">
  	<trim prefix="order by" prefixOverrides="," >
  		<if test="orderMap.id != null">
  			,id #{orderMap.id}
  		</if>
  	</trim>
  </sql>
  
  <sql id="Base_Column_List" >
    id, content, createTime
  </sql>
  
  <select id="selectByExample" resultMap="BaseResultMap" parameterType="com.manager.model.Criteria" >
    select
    <include refid="Base_Column_List" />
    from t_suggest
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
    <if test="_parameter != null" >
      <include refid="Example_Order_Clause" />
    </if>
    <include refid="common.Mysql_Pagination_Limit" />
  </select>
  
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Long" >
    select 
    <include refid="Base_Column_List" />
    from t_suggest where id = #{id}
  </select>
  
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Long" >
    delete from t_suggest where id = #{id}
  </delete>
  
  <insert id="insert" parameterType="cn.lfy.qneng.model.Suggest" useGeneratedKeys="true" keyProperty="id">
    insert into t_suggest (content, createTime)
    values (#{content}, #{createTime})
  </insert>
  
  <select id="countByExample" parameterType="com.manager.model.Criteria" resultType="java.lang.Integer" >
    select count(*) from t_suggest
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
  </select>
  
  <update id="updateByPrimaryKeySelective" parameterType="cn.lfy.qneng.model.Suggest" >
    update t_suggest
    <set >
      <if test="content != null" >
        content = #{content},
      </if>
    </set>
    where id = #{id}
  </update>
</mapper>